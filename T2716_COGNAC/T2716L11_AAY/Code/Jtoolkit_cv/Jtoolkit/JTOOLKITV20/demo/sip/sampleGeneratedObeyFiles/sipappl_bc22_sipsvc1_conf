RESET SERVER
SET SERVER PROCESSTYPE OSS
SET SERVER ARGLIST Server,8112
SET SERVER ENV CLASSPATH=/pthreads/sarala/enhan:
SET SERVER AUTORESTART 3
SET SERVER CREATEDELAY 0 SECS
SET SERVER HIGHPIN ON
SET SERVER NUMSTATIC 1 
[DO NOT CHANGE THE FOLLOWING COMMAND]
SET SERVER MAXSERVERS 1 
[DO NOT CHANGE THE FOLLOWING COMMAND]
SET SERVER MAXLINKS 16
SET SERVER STDOUT /h/sip/newlog/bc22_sipsvc1.log
SET SERVER STDERR /h/sip/newlog/bc22_sipsvc1.errlog
SET SERVER TMF ON
SET SERVER DEFINE =TCPIP^PROCESS^NAME,CLASS MAP, FILE $ZTC1J
SET SERVER DEBUG OFF
SET SERVER PRI 170
SET SERVER SECURITY "N"
SET SERVER VOLUME $data11.bc
SET SERVER CWD /h/sip/newlog
SET SERVER PROGRAM /usr/tandem/java/bin/java
SET SERVER CPUS (0)
ADD SERVER sipsvc1
