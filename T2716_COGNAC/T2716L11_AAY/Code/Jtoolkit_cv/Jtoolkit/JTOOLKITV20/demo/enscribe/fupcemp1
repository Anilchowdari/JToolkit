purge <vol>.<subvol>.cemp1 !
purge <vol>.<subvol>.cemp10 !
RESET
   SET ALTKEY ( "rn", KEYOFF 54, KEYLEN 2, FILE 0 )
   SET NO ALTCREATE
   SET ALTFILE ( 0, CEMP10 )
   SET TYPE K
   SET KEYLEN 2
   SET REC 58
   SET BLOCK 4096
   SET IBLOCK 4096
   SET EXT( 4, 32 )
   SET MAXEXTENTS 100
CREATE <vol>.<subvol>.CEMP1
   RESET
   SET TYPE K
   SET KEYLEN 6
   SET REC 6
   SET BLOCK 4096
   SET IBLOCK 4096
   SET EXT( 4, 32 )
   SET MAXEXTENTS 100
CREATE <vol>.<subvol>.CEMP10
